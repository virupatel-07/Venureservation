   <!-- Room Section Begin -->
   <section class="services-section spad">
    <div class="container">
     <div class="row">
            <div class="col-lg-12">
                <div class="contact-title">
                    <div class="section-title">
                        <span>A Memorable Event With Venue Reservation</span>
                        <h2>VENUE</h2>
                    </div>
                  
                </div>
            </div>
        </div>
         <div class="row">
        <div class="col-lg-3">
         <div class="blog-widget">
                        <div class="bw-item">
                            <h4>Categories</h4>
                            <ul>
                            <br/>
                            <% for(j=0;j<catitems.length;j++){ %>
                                <li><a href="/venue_cat/<%= catitems[j].cat_id %>""><%= catitems[j].category %></a></li><br/>
                            <% } %> 
                            
                               
                            </ul>
                        </div>
            </div>
        </div>
        <div class="col-lg-8">
        <div class="row">
            <% if(hitems.length){ %>
            <% for(i=0;i<hitems.length; i++){ %>
            <div class="col-lg-6 col-sm-6">
                <div class="services-item">
                    <% for(k=0;k<imgitems.length; k++){ %>
                        <% if(imgitems[k].hall_id == hitems[i].hall_id){ %>
                    <div class="si-pic set-bg" data-setbg="<%= imgitems[k].image_path; %>">
                        <div class="service-icon">
                            <img src="img/services/service-icon-2.png" alt="">
                            
                        </div>
                    </div>
                    <% break %>
                        <% } %>
                    <% } %>
                    <div class="si-text">
                        <h3><%= hitems[i].hall_name %></h3>
                        <a href="/customer_view_hall_detail/<%= hitems[i].hall_id %>" class="primary-btn" >VIEW DETAIL</a>
                    </div>
                </div>
            </div>
            
            <% } 
        }else{
            %>
            <h2>No Hall Detail Found</h2>
            <% } %>
           
           
          
            
        </div>

</div>	</div>
    </div>
</section>
<!-- Room Section End -->

